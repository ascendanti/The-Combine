{
  "env": {
    "FEWWORD_DISABLE": "1",
    "CLAUDE_PROJECT_DIR": "C:\\Users\\New Employee\\Downloads\\Atlas-OS-main\\Claude n8n",
    "ATLAS_ROOT": "C:\\Users\\New Employee\\Downloads\\Atlas-OS-main",
    "UTF_RESEARCH": "C:\\Users\\New Employee\\Desktop\\UTF",
    "REFERENCE_FRAMEWORKS": "C:\\Users\\New Employee\\Downloads\\Atlas-OS-main\\Reverse Engineer",
    "MALAZAN_DIR": "C:\\Users\\New Employee\\Downloads\\Atlas-OS-main\\Malazan",
    "RESOURCE_MAP": ".claude/RESOURCE-MAP.md",
    "CODEBASE_INDEX": ".claude/CODEBASE-INDEX.md",
    "EVOLUTION_PLAN": "EVOLUTION-PLAN.md",
    "DAEMON_DIR": "daemon",
    "HOOKS_DIR": ".claude/hooks",
    "SCRIPTS_DIR": ".claude/scripts",
    "AGENTS_DIR": ".claude/agents",
    "HANDOFFS_DIR": "thoughts/handoffs",
    "ORACLE_CACHE": ".claude/cache/agents/oracle",
    "NEXT_HORIZONS": ".claude/NEXT-HORIZONS.md",
    "ASCENSION_MANIFESTO": ".claude/ASCENSION-MANIFESTO.md",
    "ISSUES": ".claude/ISSUES.md",
    "TECH_TREE": ".claude/TECH-TREE.md",
    "SPECS_DIR": "specs",
    "PROFILE": "specs/ADAM-BENSAID-PROFILE.md",
    "CRM_SPEC": "specs/ATLAS-CRM-SPEC.md",
    "REVENUE_STRATEGIES": "specs/REVENUE-STRATEGIES.md",
    "RUMSFELD_MATRIX": ".claude/RUMSFELD-MATRIX.md",
    "SCIFI_ROADMAP": ".claude/SCI-FI-TECHNICAL-ROADMAP.md",
    "PRIME_DIRECTIVE": ".claude/PRIME-DIRECTIVE.md",
    "UNIFIED_ARCH": ".claude/UNIFIED-ARCHITECTURE.md",
    "STRATEGY_DB": "daemon/strategies.db",
    "OUTCOMES_DB": "daemon/outcomes.db"
  },
  "permissions": {
    "allow": [
      "Bash",
      "Edit",
      "Write",
      "Read",
      "Glob",
      "Grep",
      "Task",
      "WebSearch",
      "WebFetch",
      "Skill",
      "mcp__*"
    ],
    "defaultMode": "bypassPermissions"
  },
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python .claude/hooks/cleanup-mcp.py 2>nul || echo.",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "python .claude/hooks/session-briefing.py 2>nul || echo.",
            "timeout": 10
          },
          {
            "type": "command",
            "command": "python daemon/self_continue.py resume 2>nul || echo.",
            "timeout": 10
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python daemon/deterministic_router.py route 2>nul || echo.",
            "timeout": 3
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Read|Grep|Glob",
        "hooks": [
          {
            "type": "command",
            "command": "python .claude/hooks/auto-cache-pre.py 2>nul || echo.",
            "timeout": 2
          }
        ]
      },
      {
        "matcher": "Write",
        "hooks": [
          {
            "type": "command",
            "command": "python .claude/hooks/pre-create-check.py 2>nul || echo {\"continue\":true}",
            "timeout": 2
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Read|Grep|Glob|Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "python .claude/hooks/unified-post-tool-tracker.py 2>nul || echo.",
            "timeout": 3
          }
        ]
      },
      {
        "matcher": "Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "python .claude/hooks/post-integration-analyze.py 2>nul || echo {\"continue\":true}",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "python .claude/hooks/auto-learn-errors.py 2>nul || echo {\"continue\":true}",
            "timeout": 3
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node .claude/hooks/dist/stop-swarm-coordinator.js 2>nul || echo {\"continue\":true}",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "python .claude/scripts/pool-extractor.py 2>nul || echo.",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "python .claude/hooks/stop-memory-sync.py 2>nul || echo.",
            "timeout": 10
          },
          {
            "type": "command",
            "command": "python .claude/hooks/memory-store-stop.py 2>nul || echo.",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "python .claude/hooks/continuous-learning-stop.py 2>nul || echo.",
            "timeout": 15
          },
          {
            "type": "command",
            "command": "node .claude/hooks/dist/session-end-cleanup-swarms.js 2>nul || echo {\"continue\":true}",
            "timeout": 5
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python .claude/hooks/pre-compact-handoff.py 2>nul || echo.",
            "timeout": 15
          }
        ]
      }
    ],
    "SubagentStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node .claude/hooks/dist/subagent-start.js 2>nul || echo {\"continue\":true}",
            "timeout": 5
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node .claude/hooks/dist/subagent-stop.js 2>nul || echo {\"continue\":true}",
            "timeout": 5
          }
        ]
      }
    ]
  },
  "enabledMcpjsonServers": [
    "knowledge-graph",
    "token-optimizer",
    "tooluniverse",
    "sequential-thinking",
    "memory",
    "context7",
    "dragonfly-cache"
  ],
  "enableAllProjectMcpServers": true
}
