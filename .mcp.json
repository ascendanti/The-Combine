{
  "mcpServers": {
    "knowledge-graph": {
      "command": "mcp-knowledge-graph",
      "args": [
        "--memory-path",
        "C:/Users/New Employee/.claude/memory/knowledge-graph.jsonl"
      ],
      "env": {
        "NODE_ENV": "production"
      }
    },
    "token-optimizer": {
      "command": "npx",
      "args": [
        "-y",
        "@ooples/token-optimizer-mcp"
      ],
      "env": {
        "CACHE_DIR": "C:/Users/New Employee/.claude/cache/token-optimizer",
        "COMPRESSION_LEVEL": "high"
      }
    }
  }
}
